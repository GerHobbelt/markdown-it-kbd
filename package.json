{
  "name": "@gerhobbelt/markdown-it-kbd",
  "version": "2.2.0-7",
  "description": "Markdown-it syntax add-on for keystrokes. Renders [[x]] as <kbd>x</kbd>",
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown",
    "keystroke",
    "kbd"
  ],
  "author": {
    "name": "Joshua Gleitze",
    "url": "https://joshuagleitze.de"
  },
  "repository": "jGleitz/markdown-it-kbd",
  "license": "GPL-3.0",
  "type": "module",
  "source": "index.js",
  "main": "dist/markdownItKbd.js",
  "module": "dist/markdownItKbd.mjs",
  "types": "dist/markdownItKbd.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "make",
    "build:transpile": "babel ./src --out-dir build --extensions '.ts'",
    "build:types": "tsc --project src",
    "test": "make test",
    "unittest": "jest",
    "todo": "make todo",
    "clean": "make clean",
    "superclean": "make superclean",
    "doc": "make doc",
    "lint": "make lint",
    "lintfix": "make lintfix",
    "lint:types": "tsc",
    "lint:commits": "commitlint --from=semantic-commit-start --to=HEAD",
    "cov": "make report-coverage",
    "pub": "npm publish --access public"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.10.5",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",


    "@gerhobbelt/markdown-it": "11.0.1-43",
    "@gerhobbelt/markdown-it-attrs": "3.0.3-19",
    "@gerhobbelt/markdown-it-testgen": "0.1.6-17",
    "@gerhobbelt/markdown-it-wikilinks": "1.1.1-7",
    "@gerhobbelt/prepend-header": "1.0.8-3",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/jest": "^26.0.7",
    "@types/markdown-it": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "cross-env": "7.0.2",
    "eslint": "7.5.0",
    "jest": "26.1.0",
    "microbundle": "0.12.3",
    "mocha": "8.0.1",
    "nyc": "15.1.0",
    "semantic-release": "17.1.1",
    "typescript": "3.9.7"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# [Changelog](http://keepachangelog.com/)\n\nThis project adheres to [Semantic Versioning](http://semver.org/)."
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ]
    ]
  },
  "reporter": [
    "lcov",
    "text",
    "text-summary"
  ]
}
